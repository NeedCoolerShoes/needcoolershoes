<% content_for :scripts do %>
  <meta name="turbo-visit-control" content="reload">
<% end %>

<% if current_user.present? && current_user.id == @user.id %>
  <%= render partial: "show_owner", locals: { user: @user } %>
<% else %>
  <%= render partial: "show_visitor", locals: { user: @user } %>
<% end %>